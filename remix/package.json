{
  "name": "simeongriggs.dev-remix",
  "description": "",
  "license": "UNLICENSED",
  "scripts": {
    "build": "concurrently \"npm:build:*\"",
    "build:remix": "remix build",
    "build:css": "tailwindcss -i ./styles/global.css -o ./app/styles/global.css --minify",
    "dev": "concurrently \"npm:dev:*\" --kill-others",
    "dev:remix": "remix dev",
    "dev:css": "tailwindcss -i ./styles/global.css -o ./app/styles/global.css --watch",
    "postinstall": "remix setup node",
    "deploy": "npm run build && flyctl deploy",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.2",
    "@heroicons/react": "^1.0.5",
    "@portabletext/react": "^1.0.2",
    "@portabletext/types": "^1.0.2",
    "@remix-run/react": "^1.0.3",
    "@remix-run/serve": "^1.0.3",
    "@sanity/asset-utils": "^1.2.3",
    "@sanity/block-content-to-react": "^3.0.0",
    "@sanity/code-input": "^3.0.0-v3-studio.2",
    "@sanity/groq-store": "^0.3.1",
    "@sanity/image-url": "^1.0.1",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/typography": "^0.5.0",
    "blurhash": "^1.1.4",
    "concurrently": "^7.1.0",
    "dotenv": "^16.0.0",
    "framer-motion": "^6.2.8",
    "get-youtube-id": "^1.0.1",
    "picosanity": "^4.0.0",
    "prism-react-renderer": "^1.2.1",
    "react": "^18.2.0",
    "react-blurhash": "^0.1.3",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "remix": "^1.0.3",
    "remix-utils": "^3.3.0",
    "sanity": "^3.0.0-dev-preview.9",
    "sanity-plugin-asset-source-unsplash": "^0.2.1",
    "sanity-plugin-iframe-pane": "^1.0.16",
    "styled-components": "^5.3.5",
    "tailwindcss-fraction-widths": "^0.0.3",
    "usehooks-ts": "^2.2.1"
  },

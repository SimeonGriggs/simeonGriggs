{
  "root": true,
  "api": {
    "projectId": "az8av6xl",
    "dataset": "production"
  },
  "project": {
    "name": "simeonGriggs.dev"
  },
  "plugins": [
    "@sanity/base",
    "@sanity/default-layout",
    "@sanity/default-login",
    "@sanity/desk-tool",
    "@sanity/code-input",
    "asset-source-unsplash",
    "media"
  ],
  "env": {
    "development": {
      "plugins": ["@sanity/vision"]
    }
  },
  "parts": [
    {
      "name": "part:@sanity/base/schema",
